<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <display-name>Cab Booking Web Application</display-name>
  <servlet>
    <servlet-name>VehicleController</servlet-name>
    <servlet-class>com.omkcodes.cab_booking.controller.VehicleController</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>BookingController</servlet-name>
    <servlet-class>com.omkcodes.cab_booking.controller.BookingController</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>VehicleController</servlet-name>
    <url-pattern>/vehicle</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>BookingController</servlet-name>
    <url-pattern>/booking</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>